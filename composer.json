{
    "name": "rector/website",
    "license": "proprietary",
    "description": "Website of GetRector.org",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "erusev/parsedown-extra": "^0.8.1",
        "rector/rector": "^0.12.15",
        "symfony/asset": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/process": "^6.0",
        "symfony/templating": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "^6.0",
        "symplify/amnesia": "^10.0",
        "symplify/autowire-array-parameter": "^10.0",
        "symplify/package-builder": "^10.0",
        "symplify/smart-file-system": "^10.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "symplify/easy-testing": "^10.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symplify/coding-standard": "^10.0",
        "symplify/easy-coding-standard": "^10.0",
        "symplify/phpstan-extensions": "^10.0",
        "symplify/phpstan-rules": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Website\\": ["src", "packages"],
            "Rector\\WebsiteDemoRunner\\": "demo-runner/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Website\\Tests\\": ["tests", "packages-tests"]
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse --error-format symplify --ansi",
        "rector": "vendor/bin/rector process --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
