{
    "name": "rector/website",
    "license": "proprietary",
    "description": "Website of getrector.com",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "erusev/parsedown-extra": "^0.8.1",
        "imagine/imagine": "^1.3",
        "jajo/jsondb": "^3.0",
        "laravel/framework": "^10.1",
        "ocramius/package-versions": "^2.5",
        "rector/rector": "dev-main",
        "samsonasik/array-lookup": "^1.3",
        "spatie/laravel-markdown": "^2.2",
        "symfony/uid": "^6.2",
        "symfony/yaml": "^6.2",
        "symplify/package-builder": "^11.3",
        "symplify/vendor-patches": "^11.2",
        "tomasvotruba/punchcard": "^0.0.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4",
        "icanhazstring/composer-unused": "^0.8.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "symplify/easy-ci": "^11.2",
        "symplify/easy-coding-standard": "^11.2",
        "symplify/phpstan-extensions": "^11.2",
        "symplify/phpstan-rules": "^11.2",
        "tomasvotruba/cognitive-complexity": "^0.0.5",
        "tomasvotruba/type-coverage": "^0.0.10",
        "tomasvotruba/unused-public": "^0.1.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Rector\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Website\\Tests\\": "tests",
            "Rector\\Website\\Utils\\": "utils",
            "Rector\\Website\\Utils\\Tests\\": "utils-tests"
        },
        "classmap": [
            "stubs"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse --error-format symplify --ansi",
        "rector": "vendor/bin/rector process --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "patches": {
            "jajo/jsondb": [
                "patches/jajo-jsondb-src-jsondb-php.patch"
            ]
        }
    }
}
