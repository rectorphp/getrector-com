{
    "name": "rector/website",
    "description": "Website of GetRector.org",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "doctrine/cache": "^1.12",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.10",
        "erusev/parsedown-extra": "^0.8.1",
        "knplabs/doctrine-behaviors": "^2.3",
        "nyholm/psr7": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "rector/rector": "^0.11.58",
        "sensio/framework-extra-bundle": "^5.6",
        "sentry/sentry-symfony": "^4.2",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/doctrine-bridge": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/mailgun-mailer": "^5.4",
        "symfony/mime": "^5.4",
        "symfony/process": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/templating": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "^5.4",
        "symplify/amnesia": "^10.0",
        "symplify/autowire-array-parameter": "^10.0",
        "symplify/composer-json-manipulator": "^10.0",
        "symplify/easy-testing": "^10.0",
        "symplify/markdown-diff": "^10.0",
        "symplify/package-builder": "^10.0",
        "symplify/php-config-printer": "^10.0",
        "symplify/rule-doc-generator": "^10.0",
        "symplify/simple-php-doc-parser": "^10.0",
        "symplify/skipper": "^10.0",
        "symplify/smart-file-system": "^10.0",
        "symplify/symfony-php-config": "^10.0",
        "symplify/symplify-kernel": "^10.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/expression-language": "^5.2",
        "symplify/coding-standard": "^10.0",
        "symplify/easy-coding-standard": "^10.0",
        "symplify/phpstan-extensions": "^10.0",
        "symplify/latte-phpstan-compiler": "^10.0",
        "symplify/phpstan-rules": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Website\\": ["src", "packages"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Website\\Tests\\": ["tests", "packages-tests"]
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "doctrine": "bin/console doctrine:schema:update --dump-sql --force --ansi",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse --error-format symplify --ansi",
        "rector": "vendor/bin/rector process --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
